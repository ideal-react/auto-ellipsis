{
  "name": "auto-ellipsis",
  "version": "1.0.2",
  "description": "auto-ellipsis is a React component for truncation when content overlength.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "node server",
    "test": "node server test",
    "clean": "rimraf lib temp",
    "lint": "eslint src test --ext .js,.jsx",
    "build:lib": "babel src --out-dir lib && cp src/component/*.css lib/component",
    "build": "npm run clean && npm run lint && npm run build:lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ideal-react/auto-ellipsis.git"
  },
  "keywords": [
    "react component",
    "ellipsis",
    "truncate"
  ],
  "author": "ustccjw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ideal-react/auto-ellipsis/issues"
  },
  "homepage": "https://github.com/ideal-react/auto-ellipsis#readme",
  "devDependencies": {
    "autoprefixer": "^6.0.1",
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.16.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^3.3.1",
    "expect": "^1.10.0",
    "jsdom": "^6.3.0",
    "mocha": "^2.3.2",
    "mocha-jsdom": "^1.0.0",
    "mocha-loader": "^0.7.1",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "react": "^0.14.0-rc1",
    "react-css-modules": "git+https://git@github.com/ideal-react/react-css-modules#dev"
  }
}
